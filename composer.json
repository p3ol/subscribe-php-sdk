{
    "name": "poool/subscribe-sdk",
    "version": "1.0.0",
    "description": "Poool Subscribe SDK for PHP ðŸš€",
    "homepage": "https://github.com/p3ol/subscribe-php-sdk",
    "license": "MIT",
    "authors": [{
        "name": "Ugo Stephant",
        "email": "ugo@poool.fr",
        "homepage": "https://poool.dev"
    }],
    "require": {
        "php" : ">=8.0.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.20",
        "squizlabs/php_codesniffer": "3.6.2"
    },
    "autoload": {
        "psr-4": {
            "Poool\\Subscribe\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Poool\\Subscribe\\SDK\\Test\\": "tests"
        }
    },
    "scripts": {
        "serve": [
            "php -S localhost:62000"
        ],
        "test": [
            "XDEBUG_MODE=coverage ./vendor/bin/phpunit"
        ],
        "lint": [
            "./vendor/bin/phpcs src/ index.php"
        ]
    },
    "config": {
        "process-timeout":0
    }
}
